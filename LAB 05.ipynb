    "def vowelcounter(str):\n",
    "    count = 0\n",
    "    vowel = set (\"aeiouAEIOU\")\n",
    "    for letters in str:\n",
    "        if letters in vowel:\n",
    "            count=count+1\n",
    "            print(letters,end=',')\n",
    "    print(\"\\nNo of vowels: \",count)\n",
    "str=input(\"Enter words\")\n",
    "print(str)\n",
    "vowelcounter(str)\n",
